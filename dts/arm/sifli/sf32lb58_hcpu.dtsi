#include <arm/armv8-m.dtsi>
#include <sifli/sf32lb58_hpsys.dtsi>

/ {
    cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		hcpu: cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-m33f";
			reg = <0>;
			cpu-power-states = <&stop0 &stop1 &stop2>;
		};
    };

    sram-hcpu: memory@20000000 {
		compatible = "mmio-sram";
		reg = <0x20000000 DT_SIZE_K(2048)>;
	};

};
